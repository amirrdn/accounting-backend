{
  "name": "backend",
  "version": "1.0.0",
  "engines": {
    "node": "18.x"
  },
  "main": "dist/index.js",
  "scripts": {
    "start": "node src/index.ts",
    "dev:watch": "NODE_ENV=development nodemon --watch 'src/**/*.ts' --exec ts-node src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "npm run typeorm migration:run -- -d src/data-source.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d src/data-source.ts"
  },
  "dependencies": {
    "@types/multer": "^1.4.12",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.2",
    "mysql2": "^3.14.0",
    "pdfkit": "^0.17.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.22",
    "typescript": "^5.8.3",
    "winston": "^3.17.0",
    "ts-node": "^10.9.2" 
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.14.1",
    "@types/pdfkit": "^0.13.9",
    "nodemon": "^3.1.9"
  }
}
